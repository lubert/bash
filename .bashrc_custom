# Git aliases
alias gs="git status"
alias gch="git checkout"
alias gb="git checkout -b"
alias gg="git grep -n -I"
alias ts="tig status"

# Elastic Beanstalk aliases
alias es="eb status"

# grep
alias grep="grep --color=auto"
alias egrep="egrep --color=auto"
alias fgrep="fgrep --color=auto"

# mount
alias mount="mount |column -t"
